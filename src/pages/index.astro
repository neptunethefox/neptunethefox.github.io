---
import Layout from "../layouts/Layout.astro";
---

<Layout>
    <div class="window-body has-space">
        <h1 style="color: orange">Hi there! I am neptune!</h1>
        <p>
            This is all just a astro layout using <a
                href="https://khang-nd.github.io/7.css/">7.css</a
            >!!
        </p>

        <p>Site Completion: (Lets see if this hits 100%!)</p>
        <div
            role="progressbar"
            class="animate"
            aria-valuemin="0"
            aria-valuemax="100"
            aria-valuenow="80"
        >
            <div style="width: 0.15%"></div>
        </div><br />
    </div>
</Layout>
